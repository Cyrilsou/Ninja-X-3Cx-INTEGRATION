{
  "server": {
    "port": 3000,
    "host": "0.0.0.0"
  },
  "redis": {
    "host": "localhost",
    "port": 6379,
    "enabled": true,
    "url": "redis://localhost:6379",
    "prefix": "3cx-ninja:"
  },
  "database": {
    "dialect": "sqlite",
    "storage": "./data/database.sqlite",
    "logging": false
  },
  "whisper": {
    "model": "base",
    "modelPath": "./models",
    "pythonPath": "python",
    "language": "fr",
    "device": "cpu",
    "computeType": "int8",
    "threads": 4,
    "task": "transcribe",
    "vadFilter": true,
    "wordTimestamps": true,
    "maxConcurrent": 4
  },
  "transcription": {
    "chunkDuration": 30,
    "maxConcurrent": 2,
    "timeout": 300000,
    "maxRetries": 3,
    "retryDelay": 5000
  },
  "audio": {
    "format": "wav",
    "sampleRate": 16000,
    "channels": 1,
    "bitDepth": 16,
    "maxDuration": 1800,
    "maxSize": 104857600,
    "tempDir": "./data/temp"
  },
  "ffmpeg": {
    "path": "ffmpeg",
    "timeout": 30000
  },
  "cache": {
    "ttl": 3600,
    "checkPeriod": 600,
    "maxKeys": 1000
  },
  "webhook": {
    "timeout": 10000,
    "maxRetries": 3,
    "retryDelay": 1000
  },
  "3cx": {
    "webhook": {
      "enabled": true,
      "path": "/webhook/3cx",
      "secret": ""
    },
    "pbxUrl": "",
    "clientId": "",
    "clientSecret": ""
  },
  "ninjaone": {
    "apiUrl": "https://app.ninjarmm.com/api/v2",
    "clientId": "",
    "clientSecret": "",
    "refreshToken": "",
    "defaultBoardId": 5,
    "defaultStatusId": 1,
    "defaultPriorityId": 2
  },
  "security": {
    "apiKey": "change-this-in-production",
    "corsOrigins": ["http://localhost:3001", "http://localhost:3002"],
    "rateLimit": {
      "windowMs": 60000,
      "max": 100
    }
  },
  "cleanup": {
    "enabled": true,
    "maxAge": 86400000,
    "interval": 3600000
  },
  "logging": {
    "level": "info",
    "format": "json"
  }
}